<% include ../partials/header.ejs %>
<div class="col-xs-12 col-sm-12 col-md-10 col-md-offset-1">
    <br />
    <div class="panel panel-default">
        <div class="panel-heading">Formulaire de modification</div>
        <div class="panel-body">
            <!-- Formulaire d'inscription -->
            <form id="formModifs" enctype='application/json' class="form-horizontal">
                <div class="panel panel-default">
                    <!-- Informations générales -->
                    <div class="panel-heading">Informations générales</div>
                    <div class="panel-body">
                        <div class="form-group">
                            <label for="inputRS" class="col-sm-3 control-label">Raison Social :</label>
                            <div class="col-sm-9">
                                <input type="text" value="<%= agency.agency_name %>" class="form-control" id="inputRS" disabled>
                            </div>
                        </div>
                        <div class="form-group">
                            <label for="inputEmail" class="col-sm-3 control-label">Email de contact :</label>
                            <div class="col-sm-9">
                                <input type="email" class="form-control" id="inputEmail">
                            </div>
                        </div>
                        <div class="form-group">
                            <label for="inputPass" class="col-sm-3 control-label">Mot de passe :</label>
                            <div class="col-sm-9">
                                <input type="password" class="form-control" id="inputPass">
                            </div>
                        </div>
                        <div class="form-group">
                            <label for="inputAddr" class="col-sm-3 control-label">Adresse du siège :</label>
                            <div class="col-sm-9">
                                <input type="text" class="form-control" id="inputAddr">
                            </div>
                        </div>
                        <div class="form-group">
                            <label for="selectPays" class="col-sm-3 control-label">Pays :</label>
                            <div class="col-sm-9">
                                <select id="selectPays" class="form-control">
                                    <option value="AD">Andorra</option>
                                    <option value="AR">Argentina</option>
                                    <option value="AS">American Samoa</option>
                                    <option value="AD">Andorra</option>
                                    <option value="AR">Argentina</option>
                                    <option value="AS">American Samoa</option>
                                    <option value="AT">Austria</option>
                                    <option value="AU">Australia</option>
                                    <option value="BD">Bangladesh</option>
                                    <option value="BE">Belgium</option>
                                    <option value="BG">Bulgaria</option>
                                    <option value="BR">Brazil</option>
                                    <option value="CA">Canada</option>
                                    <option value="CH">Switzerland</option>
                                    <option value="CZ">Czech Republic</option>
                                    <option value="DE">Germany</option>
                                    <option value="DK">Denmark</option>
                                    <option value="DO">Dominican Republic</option>
                                    <option value="ES">Spain</option>
                                    <option value="FI">Finland</option>
                                    <option value="FO">Faroe Islands</option>
                                    <option value="FR">France</option>
                                    <option value="GB">Great Britain</option>
                                    <option value="GF">French Guyana</option>
                                    <option value="GG">Guernsey</option>
                                    <option value="GL">Greenland</option>
                                    <option value="GP">Guadeloupe</option>
                                    <option value="GT">Guatemala</option>
                                    <option value="GU">Guam</option>
                                    <option value="GY">Guyana</option>
                                    <option value="HR">Croatia</option>
                                    <option value="HU">Hungary</option>
                                    <option value="IM">Isle of Man</option>
                                    <option value="IN">India</option>
                                    <option value="IS">Iceland</option>
                                    <option value="IT">Italy</option>
                                    <option value="JE">Jersey</option>
                                    <option value="JP">Japan</option>
                                    <option value="LI">Liechtenstein</option>
                                    <option value="LK">Sri Lanka</option>
                                    <option value="LT">Lithuania</option>
                                    <option value="LU">Luxembourg</option>
                                    <option value="MC">Monaco</option>
                                    <option value="MD">Moldavia</option>
                                    <option value="MH">Marshall Islands</option>
                                    <option value="MK">Macedonia</option>
                                    <option value="MP">Northern Mariana Islands</option>
                                    <option value="MQ">Martinique</option>
                                    <option value="MX">Mexico</option>
                                    <option value="MY">Malaysia</option>
                                    <option value="NL">Holland</option>
                                    <option value="NO">Norway</option>
                                    <option value="NZ">New Zealand</option>
                                    <option value="PH">Phillippines</option>
                                    <option value="PK">Pakistan</option>
                                    <option value="PL">Poland</option>
                                    <option value="PM">Saint Pierre and Miquelon</option>
                                    <option value="PR">Puerto Rico</option>
                                    <option value="PT">Portugal</option>
                                    <option value="RE">French Reunion</option>
                                    <option value="RU">Russia</option>
                                    <option value="SE">Sweden</option>
                                    <option value="SI">Slovenia</option>
                                    <option value="SJ">Svalbard & Jan Mayen Islands</option>
                                    <option value="SK">Slovak Republic</option>
                                    <option value="SM">San Marino</option>
                                    <option value="TH">Thailand</option>
                                    <option value="TR">Turkey</option>
                                    <option value="US">United States</option>
                                    <option value="VA">Vatican</option>
                                    <option value="VI">Virgin Islands</option>
                                    <option value="YT">Mayotte</option>
                                    <option value="ZA">South Africa</option>
                                </select>
                            </div>
                        </div>

                        <div class="form-group">
                            <label for="inputCP" class="col-sm-3 control-label">Code Postal :</label>
                            <div class="col-sm-9">
                                <input type="text" class="form-control" id="inputCP" placeholder="Tapez un code postal pour avoir la liste des villes correspondante">
                            </div>
                        </div>
                        <div class="form-group">
                            <label for="inputVille" class="col-sm-3 control-label">Ville :</label>
                            <div class="col-sm-9">
                                <select id="selectVille" class="form-control" />
                                </select>
                            </div>
                        </div>
                        <div class="form-group">
                            <label for="inputSite" class="col-sm-3 control-label">Site web de la compagnie :</label>
                            <div class="col-sm-9">
                                <input type="url" value="<%= agency.agency_url %>" class="form-control" id="inputSite" placeholder="" disabled>
                            </div>
                        </div>
                        <div class="form-group">
                            <label for="inputTel" class="col-sm-3 control-label">Numéro de téléphone :</label>
                            <div class="col-sm-9">
                                <input type="tel" class="form-control" value="<%= agency.agency_phone %>" id="inputTel" placeholder="" disabled>
                            </div>
                        </div>
                    </div>
                </div>
                <!-- Informations téchniques -->
                <div class="panel panel-default">
                    <div class="panel-heading">GTFS</div>
                    <div class="panel-body">
                        <div class="form-group">
                            <label for="inputGTFSZipFile" class="col-sm-3 control-label">Adresse du fichier zip contenant tous les fichiers GTFS :</label>
                            <div class="col-sm-9">
                                <input type="url" class="form-control" value="<%= agency.urlGTFSFile %>" id="inputGTFSZipFile" placeholder="http://host/fichier.zip">
                            </div>
                        </div>
                        <div class="form-group">
                            <label for="inputUseGTFSRealTime" class="col-sm-3 control-label">Utiliser GTFS-Realtime :</label>
                            <div class="col-sm-9">
                                <input type="checkbox" class="form-control" id="inputUseGTFSRealTime">
                            </div>
                        </div>
                        <div class="form-group">
                            <label for="inputGTFSRealTimeTripUpdate" class="col-sm-3 control-label">Adresse du fichiers "TripUpdates" : </label>
                            <div class="col-sm-9">
                                <input type="url" class="form-control" value="<%= agency.urlGTFSTripUpdate %>" id="inputGTFSRealTimeTripUpdate" placeholder="http://host:port/TripUpdate.pb">
                            </div>
                        </div>
                        <div class="form-group">
                            <label for="inputGTFSRealTimeAlert" class="col-sm-3 control-label">Adresse du fichiers "Alert" : </label>
                            <div class="col-sm-9">
                                <input type="url" class="form-control" value="<%= agency.urlGTFSAlert %>" id="inputGTFSRealTimeAlert" placeholder="http://host:port/Alert.pb">
                            </div>
                        </div>
                        <div class="form-group">
                            <label for="inputGTFSRealVehiclePosition" class="col-sm-3 control-label">Adresse du fichiers "VehiclePosition" : </label>
                            <div class="col-sm-9">
                                <input type="url" class="form-control" value="<%= agency.urlGTFSVehiclePosition %>" id="inputGTFSRealVehiclePosition" placeholder="http://host:port/VehiclePosition.pb">
                            </div>
                        </div>
                    </div>
                </div>
                <button type="submit" id="submitInscription" class="pull-right btn btn-default">Enregistrer les modifications</button>
                <!-- Zone de retour pour l'utilisateur -->
                <div id="messageInfo" class="bg-info hidden">

                </div>
            </form>
        </div>
    </div>
</div>
<% include ../partials/footer.ejs %>